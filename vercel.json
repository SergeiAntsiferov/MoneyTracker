{
    "version": 1,
    "builds": [
      {
        "src": "./backend/app.mjs",
        "use": "@vercel/node"
      },
      {
        "src": "./frontend/build",  
        "use": "@vercel/static"
      }
    ],
  
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/backend/app.mjs"
      }
    ]
}